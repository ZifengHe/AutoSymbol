<Window x:Class="AutoSymbol.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:AutoSymbol"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800">
    <DockPanel LastChildFill="True">
        <StackPanel Orientation="Vertical" DockPanel.Dock="Top">
            <ToolBarTray >
                <ToolBar>
                    <Button Content="Run"  Click="RunClicked"/>
                    <Button Command="Delete" Content="Delete" />
                    <Button Command="Refresh" Content="Refresh" />
                </ToolBar>
            </ToolBarTray>
        </StackPanel>
        <StatusBar Name="statusbar" DockPanel.Dock="Bottom">statusbar</StatusBar>
        <DockPanel x:Name="ContentPanel"></DockPanel>
    </DockPanel>
</Window>
